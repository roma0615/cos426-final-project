{
    "$schema": "https://json.schemastore.org/package.json",
    "repository": "cos426-final-project",
    "name": "aliens-in-cahoots",
    "type": "module",
    "version": "0.1.0",
    "title": "Aliens in Cahoots",
    "description": "You control two Aliens in Cahoots. They have to get back to their landing pads to get back to their spaceship. Will you be able to manipulate gravity to your will, so they can make it back home safely?",
    "keywords": [
        "Princeton",
        "graphics",
        "three.js",
        "COS 426",
        "gravity",
        "two-player",
        "platformer"
    ],
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "prettier --write ."
    },
    "dependencies": {
        "cannon-es": "^0.20.0",
        "dat.gui": "^0.7.9",
        "three": "^0.158.0",
        "three-to-cannon": "^4.3.0"
    },
    "devDependencies": {
        "@types/dat.gui": "^0.7.12",
        "@types/three": "^0.158.3",
        "prettier": "^3.1.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-darwin-arm64": "4.6.1"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "semi": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "printWidth": 80
    }
}
